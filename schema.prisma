datasource db {
    provider = "sqlite"
    url      = "file:./db/database.sqlite3"
}

generator client {
    provider = "prisma-client-js"
}

model InviteLink {
    url  String @id
    time BigInt
}

model SettingsExcludeBot {
    guildId String @id
}

model UserSetting {
    userId     String @id
    msgReplace Boolean?
}
